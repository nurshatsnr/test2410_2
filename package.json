  "scripts": {
    "serverless": "serverless",
    "build": "webpack --config webpack.config.js",
    "nodeploy": "serverless deploy -n",
    "deploy-dev": "serverless deploy --stage=dev --aws-profile=smsjourney",
    "deploy-qa": "serverless deploy --stage=qa --aws-profile=smsjourney",
    "deploy-prod": "serverless deploy --stage=prod --aws-profile=smsjourney",
    "remove-prod": "serverless remove --stage=prod --aws-profile=smsjourney",
    "remove-dev": "serverless remove --stage=dev --aws-profile=smsjourney",
    "remove-qa": "serverless remove --stage=qa --aws-profile=smsjourney",
    "lint": "tslint \"src/**/*.ts\" --force",
    "test:serverless": "npm run build && serverless invoke local -f Homeork -p test/addHomework.json -s dev",
    "offline": "yarn serverless offline  --stage=dev --aws-profile=smsjourney",
    "local": "serverless offline start --stage=local --aws-profile=smsjourney",
    "integration-tests": "mocha --timeout 40000 --exit test/integrationTests.js",
    "func-dev": "serverless deploy function -f importApiFile --stage=dev --aws-profile=smsjourney",
    "func-qa": "serverless deploy function -f importApiFile --stage=qa --aws-profile=smsjourney",
    "func-prod": "serverless deploy function -f importApiFile --stage=prod --aws-profile=smsjourney",
    "test": "jest"
  },
